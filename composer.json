{
  "require": {
    "barrelstrength/sprout-encode-email": "2.0.5",
    "craftcms/ckeditor": "1.0.0-beta.2",
    "craftcms/cms": "3.1.21.1",
    "craftcms/redactor": "2.3.2",
    "mmikkel/cp-field-inspect": "1.0.5",
    "verbb/cp-nav": "2.0.9",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
